tail -f /var/log/capture.log

cd /var/log/capture/
ls -lrt
tail -f alerts-*

ifconfig | grep PROMISC
tcpdump -i netmap:<>/r

netstat -na | grep 12106 (Established)

service blu_naf stop
service blu_paf stop
service blu_flows stop



Collector Side:

systemctl status naf paf naf-conn1 naf-conn2 netflow windows
netstat -na | grep 12106  (Listening)
sudo systemctl stop paf
nc -lv 12106
sudo systemctl start paf
sudo systemctl status paf
 
du -hd 1 .

